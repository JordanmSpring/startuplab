
angular.module('StartupLab').directive("slFounders", function() {
  return {
    restrict: 'E',
    templateUrl: '<%= asset_path("templates/founders/modal.html") %>',
    controller: 'FoundersController',
    scope: {
      idea: '=ngModel',
      linkTitle: '@'
    },
    link: function(scope, element) {
      scope.modal = element.find('.modal');
    }
  }
});
